.body{
  margin-top: 28rpx;
  width: 700rpx;
  height: 150rpx;
  background: #FFFFFF;
  opacity: 1;
  border-radius: 12rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.divLine{
  background: #F9F9F9;
  width: 652rpx;
  height: 2rpx;
  margin: auto;
 }
 .list{
  height: 80rpx;
  display: flex;
  align-items: center;
 }
.list1{
  display: flex;
  align-items: center;
  height: 40rpx;
  margin-left: 26rpx;
}
.list_text{
  font-size: 28rpx;
  margin-left: 10rpx;
}
.list_img{
  width: 12rpx;
  height: 12rpx;
}
.listbottom{
  height: 80rpx;
  display: flex;
  align-items: center;
}
.election{
  height: 44rpx;
  width: 620rpx;
  margin-left: 26rpx;
  display: flex;
  justify-content: start;
}
.picker{
  width: 600rpx;
  font-size: 32rpx;
  color: #555555;
  font-family: PingFang SC;
}
.election_img{
  width: 15rpx;
  height: 24rpx;
  margin-top: 10rpx;
}
.terpho{
  margin-left: 25rpx;
}
.terpho_in{
  border: 1rpx solid #A4A4A6;
  color: #A4A4A6;
  width: 550rpx;
  height: 64rpx;
  border-radius: 8rpx;
  padding-left: 20rpx;
}
.terpho_but{
  color:#FFFFFF;
  width: 140rpx;
  height: 64rpx;
  font-size: 28rpx;
  line-height: 64rpx;
  text-align: center;
  background-color: #7ACBB3;
  border-radius: 8rpx;
  margin-left: 48rpx;
}
.bottom{
  margin-top: 28rpx;
  width: 700rpx;
  height: 400rpx;
  background-color: #FFFFFF;
  border-radius: 12rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.message{
  height: 40rpx;
  font-size: 28rpx;
  color: #333333;
  line-height: 40rpx;
  margin-left: 28rpx;
}
.but{
  padding: 0;
  background-color: #7ACBB3;
  color: #FFFFFF;
  font-size: 28rpx;
  font-weight: 400;
  margin-bottom: 80rpx;
}