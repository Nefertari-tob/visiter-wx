.head{
  width: 700rpx;
  height: 322rpx;
  margin-top: 24rpx;
  border-radius: 12rpx;
  background-color: #FFFFFF;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.head-top{
  display: flex;
  justify-content: space-between;
}
.user{
  height: 322rpx;
  display: flex;
  align-items: center;
}
.user-massage{
  width: 270rpx;
  height: 260rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.userinfo-avatar {
  width: 156rpx;
  height: 222rpx;
  margin-left: 24rpx;
  margin-right: 24rpx;
  opacity: 1;
  border-radius: 8rpx;
}
.userinfo-nickname {
  height: 50rpx;
  display: flex;
  font-size: 32rpx;
  flex-direction: column;
  justify-content: space-around;
  color: #666666;
}
.choice{
  font-size: 28rpx;
  color: #666666;
}
