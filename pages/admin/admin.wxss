.zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: #000000; 
  display: none;
  /* background-color: #888888; */
  opacity:0.4
}

.zan-dialog__container {
  position: fixed;
  top: 444rpx;
  width: 560rpx;
  height: 446rpx;
  margin-left: -285rpx;
  background: #FFFFFF;
  transform: translateY(240%);
  transition: all 0.4s ease;
  z-index: 12;
  border-radius: 20rpx;
  /* box-shadow: 0px 3px 3px 2px gainsboro; */
}

.zan-dialog--show .zan-dialog__container {
  transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
  display: block;
}
